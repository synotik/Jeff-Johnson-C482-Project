<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="modifyPartAnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Jeff_JohnsonC482_Project.View_Controller.ModifyPartScreenController">
    <children>
        <VBox fx:id="modifyPartLabelVbox" alignment="CENTER_LEFT" layoutX="170.0" layoutY="81.0" spacing="20.0">
            <children>
                <Label fx:id="modifyPartIdLabel" text="ID" />
                <Label fx:id="modifyPartNameLabel" text="Name" />
                <Label fx:id="modifyPartInvLabel" text="Inv" />
                <Label fx:id="modifyPartPriceCostLabel" text="Price/Cost" />
                <Label fx:id="modifyPartMinLabel" text="Min" />
                <Label fx:id="modifyPartMaxLabel" text="Max" />
                <Label fx:id="modifyPartChangingLabel" text="Machine ID" />
            </children>
        </VBox>
        <VBox fx:id="modifyPartTextBoxVbox" alignment="CENTER_LEFT" layoutX="300.0" layoutY="77.0" spacing="14.0">
            <children>
            <Label fx:id="modifyPartIdLabelChanger" />
                <TextField fx:id="modifyPartNameTextBox" promptText="Part Name">
                    <font>
                        <Font name="System Italic" size="12.0" />
                    </font>
                </TextField>
                <TextField fx:id="modifyPartInvTextBox" promptText="Inv">
                    <font>
                        <Font name="System Italic" size="12.0" />
                    </font>
                </TextField>
                <TextField fx:id="modifyPartPriceCostTextBox" promptText="Price/Cost">
                    <font>
                        <Font name="System Italic" size="12.0" />
                    </font>
                </TextField>
                <TextField fx:id="modifyPartMinTextBox" promptText="Min">
                    <font>
                        <Font name="System Italic" size="12.0" />
                    </font>
                </TextField>
                <TextField fx:id="modifyPartMaxTextBox" promptText="Max">
                    <font>
                        <Font name="System Italic" size="12.0" />
                    </font>
                </TextField>
                <TextField fx:id="modifyPartChangingTextBox" promptText="Machine ID">
                    <font>
                        <Font name="System Italic" size="12.0" />
                    </font>
                </TextField>
            </children>
        </VBox>
        <Label fx:id="modifyPartLabel" layoutX="14.0" layoutY="14.0" text="Modify Part">
            <font>
                <Font size="22.0" />
            </font>
        </Label>
        <RadioButton fx:id="modifyPartInhouseRadioButton" layoutX="201.0" layoutY="22.0" mnemonicParsing="false" text="In-House" />
        <RadioButton fx:id="modifyPartOutsourcedRadioButton" layoutX="307.0" layoutY="22.0" mnemonicParsing="false" text="Outsourced" />
        <HBox fx:id="modifyPartButtonHbox" alignment="CENTER" layoutX="349.0" layoutY="348.0" spacing="10.0">
            <children>
                <Button fx:id="modifyPartSaveButton" mnemonicParsing="false" onAction="#handleSaveButton" text="Save" />
                <Button fx:id="modifyPartCancelButton" mnemonicParsing="false" onAction="#handleCancelButton" text="Cancel" />
            </children>
        </HBox>
    </children>
</AnchorPane>
