<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="modifyProductAnchorPane" prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Jeff_JohnsonC482_Project.View_Controller.ModifyProductScreenController">
    <children>
        <VBox fx:id="modifyProductLabelVbox" alignment="CENTER_LEFT" layoutX="35.0" layoutY="185.0" spacing="20.0">
            <children>
                <Label fx:id="modifyProductIdLabel" text="ID" />
                <Label fx:id="modifyProductNameLabel" text="Name" />
                <Label fx:id="modifyProductInvLabel" text="Inv" />
                <Label fx:id="modifyProductPriceLabel" text="Price" />
                <Label fx:id="modifyProductMinLabel" text="Min" />
                <Label fx:id="modifyProductMaxLabel" text="Max" />
            </children>
        </VBox>
        <VBox fx:id="modifyProductTextFieldVbox" layoutX="114.0" layoutY="186.0" spacing="12.0">
            <children>
                <TextField fx:id="modifyProductIdTextField" promptText="Product ID">
               <font>
                  <Font name="System Italic" size="12.0" />
               </font></TextField>
                <TextField fx:id="modifyProductNameTextField" promptText="Product Name">
               <font>
                  <Font name="System Italic" size="12.0" />
               </font></TextField>
                <TextField fx:id="modifyProductInvTextField" promptText="Inv">
               <font>
                  <Font name="System Italic" size="12.0" />
               </font></TextField>
                <TextField fx:id="modifyProductPriceTextField" promptText="Price">
               <font>
                  <Font name="System Italic" size="12.0" />
               </font></TextField>
                <TextField fx:id="modifyProductMinTextField" promptText="Min">
               <font>
                  <Font name="System Italic" size="12.0" />
               </font></TextField>
                <TextField fx:id="modifyProductMaxTextField" promptText="Max">
               <font>
                  <Font name="System Italic" size="12.0" />
               </font></TextField>
            </children>
        </VBox>
        <TableView fx:id="modifyProductExistingTableView" layoutX="410.0" layoutY="61.0" prefHeight="200.0" prefWidth="555.0">
            <columns>
                <TableColumn fx:id="modifyProductNewPartIdColumn" minWidth="-Infinity" prefWidth="75.0" text="Part ID" />
                <TableColumn fx:id="modifyProductNewPartNameColumn" minWidth="-Infinity" prefWidth="75.0" text="Part Name" />
                <TableColumn fx:id="modifyProductInventoryLevelColumn" prefWidth="100.0" text="Inventory Level" />
                <TableColumn fx:id="modifyProductPricePerUnitColumn" prefWidth="100.0" text="Price Per Unit" />
            </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
        </TableView>
        <TableView fx:id="modifyProductNewTableView" layoutX="410.0" layoutY="315.0" prefHeight="200.0" prefWidth="555.0">
            <columns>
                <TableColumn fx:id="modifyProductExistingPartIdColumn" minWidth="-Infinity" prefWidth="75.0" text="Part ID" />
                <TableColumn fx:id="modifyProductExistingPartNameColumn" minWidth="-Infinity" prefWidth="75.0" text="Part Name" />
                <TableColumn fx:id="modifyProductExistingInventoryLevelColumn" prefWidth="100.0" text="Inventory Level" />
                <TableColumn fx:id="modifyProductExistingPricePerUnitColumn" prefWidth="100.0" text="Price Per Unit" />
            </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
        </TableView>
        <Button fx:id="modifyProductAddButton" layoutX="927.0" layoutY="274.0" mnemonicParsing="false" onAction="#handleAddButton" text="Add" />
        <Button fx:id="modifyProductDeleteButton" layoutX="914.0" layoutY="527.0" mnemonicParsing="false" onAction="#handleDeleteButton" text="Delete" />
        <Button fx:id="modifyProductCancelButton" layoutX="914.0" layoutY="561.0" mnemonicParsing="false" onAction="#handleCancelButton" text="Cancel" />
        <Button fx:id="modifyProductSaveButton" layoutX="863.0" layoutY="561.0" mnemonicParsing="false" onAction="#handleSaveButton" text="Save" />
        <Button fx:id="modifyProductSearchButton" layoutX="636.0" layoutY="23.0" mnemonicParsing="false" onAction="#handleSearchButton" text="Search" />
        <TextField fx:id="modifyProductSearchTextField" layoutX="695.0" layoutY="23.0" prefHeight="25.0" prefWidth="270.0" />
        <Label fx:id="modifyProductLabel" layoutX="49.0" layoutY="45.0" text="Modify Product">
            <font>
                <Font name="System Bold" size="22.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
